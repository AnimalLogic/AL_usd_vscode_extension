{
    "name": "usd",
    "displayName": "USD for VSCode",
    "description": "Pixar USD Language Extension from Animal Logic",
    "version": "0.0.2",
    "publisher": "animallogic-vscode",
    "keywords": ["pixar", "usd", "usda"],
    "repository": {
        "type": "git",
        "url": "https://github.al.com.au/rnd/AL_usd_vscode_extension"
    },
    "icon": "images/USDLogoHeader.png",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "usd",
            "aliases": ["USD", "usd"],
            "extensions": [".usd",".usda"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "usd",
            "scopeName": "source.usd",
            "path": "./syntaxes/usd.tmLanguage.json"
        }]
    }
}